; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="jellism"
run/main_scene="res://World.tscn"
config/features=PackedStringArray("4.5", "C#", "Mobile")
config/icon="res://icon.svg"

[dotnet]

project/assembly_name="jellism"

[editor_plugins]

enabled=PackedStringArray("res://addons/ai_architect/plugin.cfg", "res://addons/ai_assistant_hub/plugin.cfg")

[layer_names]

2d_physics/layer_1="membrane"
2d_physics/layer_2="nucleus"
2d_physics/layer_3="food"
2d_physics/layer_4="cell_core"

[physics]

2d/solver/solver_iterations=30

[plugins]

ai_assistant_hub/llm_api="gemini_api"
ai_assistant_hub/preferences/thinking_target=0
ai_assistant_hub/preferences/skip_greeting=false
ai_assistant_hub/preferences/always_scroll_to_bottom=false

[rendering]

renderer/rendering_method="mobile"
